@PACKAGE_INIT@

# Required so that on windows Release and RelWithDebInfo can be used instead of
# default fallback which is Debug See
# https://gitlab.kitware.com/cmake/cmake/issues/20319

set(CMAKE_MAP_IMPORTED_CONFIG_MINSIZEREL MinSizeRel RelWithDebInfo Release
                                         Debug "")
set(CMAKE_MAP_IMPORTED_CONFIG_RELWITHDEBINFO RelWithDebInfo Release MinSizeRel
                                             Debug "")
set(CMAKE_MAP_IMPORTED_CONFIG_RELEASE Release RelWithDebInfo MinSizeRel Debug
                                      "")

# If your package depends an another one, you MUST specify it here
include(CMakeFindDependencyMacro)
find_dependency(spdlog REQUIRED)
find_dependency(absl REQUIRED)

include("${CMAKE_CURRENT_LIST_DIR}/@PROJECT_NAME@Targets.cmake")
